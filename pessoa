import java.time.LocalDate;

public class Pessoa {

    private String nome;
    private LocalDate dataNascimento;
    private int idade;
    private String cpf;
    private String genero;
    
    
    //Construtor
    public Pessoa(String nome, LocalDate dataNascimento, int idade, String cpf){
        this.nome = nome;
        this.dataNascimento = dataNascimento;
        this.idade = idade;
        this.cpf = cpf;
        this.genero = genero;
}
    //Get & SetÂ´s
    public String getNome(){
        return nome;
    }
    public void setNome(){
        this.nome = nome;
    }
    public LocalDate getDataNascimento(){
        return dataNascimento;
    }
    public void setDataNascimento(){
        this.dataNascimento = dataNascimento;
    }
    public int getIdade(){
        return idade;
    }
    public void setIdade(int idade){
        this.idade = idade;
    }
    public String getCpf(){
        return cpf;
    }
    public void setCpf(){
        this.cpf = cpf;
    }
    public String getGenero(){
        return genero;
    }
    public void setGenero(){
        this.genero = genero;
    }
}

    public class Aluno extends Pessoa{
        private String telefone, genero;
        
        public Aluno(String nome, LocalDate dataNascimento, int idade, String cpf, String telefone, String genero){
            super(nome, dataNascimento, idade, cpf);
            this.telefone = telefone;
        }
        
        //get's & set's
        public String getTelefone(){
        return telefone;
     }
        public void setTelefone(){
        this.telefone = telefone;
    }
        
        
    public class Professor extends Pessoa{
        private String anosExperiencia, email;

        public Professor(String nome, LocalDate dataNascimento, int idade, String cpf, String telefone, String genero, String anosExperiencia, String email) {
            super(nome, dataNascimento, idade, cpf);
            this.anosExperiencia = anosExperiencia;
            this.email = email;
            
        }
    }
}
